{
  "name": "esfile",
  "publisher": "Minieyes",
  "displayName": "esfile",
  "description": "use es.exe to manipulate files",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
	"repository": {
		"type": "git",
		"url": "https://github.com/minieyes0708/VSCodeExension-ESFile"
	},
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "minieyes.esfile.openfile",
        "title": "Minieyes ESFile: Open File"
      },
      {
        "command": "minieyes.bookmarks.openfile",
        "title": "Minieyes Bookmarks: Open File"
      },
      {
        "command": "minieyes.edit.todo",
        "title": "Minieyes Edit: TODO.md"
      },
      {
        "command": "minieyes.edit.programs",
        "title": "Minieyes Edit: programs.txt"
      },
      {
        "command": "minieyes.edit.bookmarks",
        "title": "Minieyes Edit: bookmarks.txt"
      }
    ],
    "configuration": [
      {
        "title": "Bookmarks",
        "properties": {
          "minieyes.esfile.excludeFolders": {
            "type": ["array"],
            "default": [],
            "description": "folders to exclude while es.exe listing files"
          },
          "minieyes.bookmarks.path": {
            "type": ["string"],
            "default": [],
            "description": "bookmark.txt path"
          },
          "minieyes.bookmarks.expandFolders": {
            "type": ["array"],
            "default": [],
            "description": "expand further paths into these bookmarks"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "iconv-lite": "^0.6.3"
  }
}
